@prefix dbp: <http://dbpedia.org/resource/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lang: <http://id.loc.gov/vocabulary/iso639-1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# UPGRADE: can we change this dynamically if base_uri is different?
@base <http://localhost:8080/> .

<fdp> a dcterms:Agent ;
    rdfs:label "FAIR Data Point of the Plant Breeding Group, Wageningen UR"^^xsd:string ;
    dcterms:description "This FDP provides metadata on plant-specific genotype/phenotype data sets"^^xsd:string ;
    dcterms:hasPart <catalog/catalog-01> ;
    dcterms:hasVersion "1.0"^^xsd:string ;
    dcterms:identifier "FDP-WUR-PB"^^xsd:string ;
    dcterms:issued "2015-11-24"^^xsd:date ;
    dcterms:language lang:en ;
    dcterms:modified "2015-11-24"^^xsd:date ;
    dcterms:publisher <http://orcid.org/0000-0002-4368-8058> ;
    dcterms:title "FAIR Data Point of the Plant Breeding Group, Wageningen UR"^^xsd:string ;
    rdfs:seeAlso <catalog/catalog-01> .

<catalog/catalog-01> a dcat:Catalog ;
    rdfs:label "Plant Breeding Data Catalog"^^xsd:string ;
    dcterms:description "Plant Breeding Data Catalog"^^xsd:string ;
    dcterms:hasVersion "1.0"^^xsd:string ;
    dcterms:identifier "catalog-01"^^xsd:string ;
    dcterms:issued "2015-11-24"^^xsd:date ;
    dcterms:language lang:en ;
    dcterms:modified "2015-11-24"^^xsd:date ;
    dcterms:publisher <http://orcid.org/0000-0002-4368-8058> ;
    dcterms:title "Plant Breeding Data Catalog"^^xsd:string ;
    rdfs:seeAlso <dataset/breedb> ;
    dcat:dataset <dataset/breedb> ;
    dcat:themeTaxonomy dbp:Breeding .

<dataset/breedb> a dcmitype:Dataset,
        dcat:Dataset ;
    rdfs:label "BreeDB tomato passport data"^^xsd:string ;
    dcterms:description "BreeDB tomato passport data"^^xsd:string ;
    dcterms:hasVersion "1.0"^^xsd:string ;
    dcterms:identifier "breedb"^^xsd:string ;
    dcterms:issued "2015-11-24"^^xsd:date ;
    dcterms:language lang:en ;
    dcterms:modified "2015-11-24"^^xsd:date ;
    dcterms:publisher <http://orcid.org/0000-0002-4368-8058> ;
    dcterms:title "BreeDB tomato passport data"^^xsd:string ;
    rdfs:seeAlso <distribution/breedb-sparql>,
        <distribution/breedb-sqldump> ;
    dcat:distribution <distribution/breedb-sparql>,
        <distribution/breedb-sqldump> ;
    dcat:keyword "BreeDB"^^xsd:string,
        "Plant breeding"^^xsd:string,
        "germplasm"^^xsd:string,
        "passport data"^^xsd:string ;
    dcat:landingPage <http://www.eu-sol.wur.nl/passport> ;
    dcat:theme dbp:Plant_breeding .

<distribution/breedb-sparql> a dcat:Distribution ;
    rdfs:label "SPARQL endpoint for BreeDB tomato passport data"^^xsd:string ;
    dcterms:description "SPARQL endpoint for BreeDB tomato passport data"^^xsd:string ;
    dcterms:hasVersion "1.0"^^xsd:string ;
    dcterms:identifier "breedb-sparql"^^xsd:string ;
    dcterms:issued "2015-11-24"^^xsd:date ;
    dcterms:language lang:en ;
    dcterms:license <http://rdflicense.appspot.com/rdflicense/cc-by-nc-nd3.0> ;
    dcterms:modified "2015-11-24"^^xsd:date ;
    dcterms:publisher <http://orcid.org/0000-0002-4368-8058> ;
    dcterms:title "SPARQL endpoint for BreeDB tomato passport data"^^xsd:string ;
    dcat:accessURL <http://virtuoso.biotools.nl:8888/sparql> ;
    dcat:mediaType "application/n-triples"^^xsd:string,
        "application/rdf+xml"^^xsd:string .
